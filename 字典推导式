import random
randomdict = {i:random.randint(10,100) for i in range(1,5)}
print(randomdict)

print('\n')

name = ['绮梦','冷伊一','香凝','黛兰']     #保存名字的列表
sign = ['水瓶座','射手座','双鱼座','双子座']  #保存星座的列表
dict1 = {i:j for i,j in zip(name,sign)}           #通过zip()组成新的元组,此处组合成了字典推导式
print(dict1)

print(tuple(zip(name,sign)))       #查看zip()的组成情况
